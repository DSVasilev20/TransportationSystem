        <servlet-name>addPackageServlet</servlet-name>
        <url-pattern>/add-package-servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>editPackageServlet</servlet-name>
        <servlet-class>EditPackageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editPackageServlet</servlet-name>
        <url-pattern>/edit-package-servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addLocationServlet</servlet-name>
        <servlet-class>AddLocationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editLocationServlet</servlet-name>
        <url-pattern>/edit-location-servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>editLocationServlet</servlet-name>
        <servlet-class>EditLocationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addLocationServlet</servlet-name>
        <url-pattern>/add-location-servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addTransportationVehicleServlet</servlet-name>
