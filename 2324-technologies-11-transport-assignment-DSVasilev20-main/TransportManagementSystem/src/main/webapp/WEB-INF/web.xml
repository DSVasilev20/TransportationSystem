        <url-pattern>/add-user-servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>editUserServlet</servlet-name>
        <servlet-class>EditUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editUserServlet</servlet-name>
        <url-pattern>/edit-user-servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addPackageServlet</servlet-name>
