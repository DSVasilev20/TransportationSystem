        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login-servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>logoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logoutServlet</servlet-name>
        <url-pattern>/logout-servlet</url-pattern>
    </servlet-mapping>
</web-app>
